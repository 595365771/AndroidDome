<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blackcat.example">

    <application
        android:name="com.blackcat.example.app.BlackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.blackcat.example.ui.touchevent.TouchEventActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.blackcat.example.ui.thread.ThreadOneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.blackcat.example.ui.thread.ThreadTwoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.blackcat.example.ui.thread.ThreadThreeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.blackcat.example.ui.service.ServiceOneActivity"
            android:screenOrientation="portrait" />
       <service android:name="com.blackcat.example.service.ServiceTest"/>
    </application>

</manifest>